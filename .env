# Paramètres de connexion à la base de données MariaDB
DB_HOST=localhost
DB_PORT=3306
DB_NAME=votre_base_de_donnees
DB_USER=votre_utilisateur
DB_PASSWORD=votre_mot_de_passe

# Paramètres optionnels
DB_CHARSET=utf8mb4
DB_COLLATION=utf8mb4_unicode_ci

# Paramètres de sécurité
MARIADB_ROOT_PASSWORD=mot_de_passe_root_tres_securise

# Paramètres optionnels pour Docker
MARIADB_ALLOW_EMPTY_ROOT_PASSWORD=no
MARIADB_RANDOM_ROOT_PASSWORD=no

# Autres paramètres de l'application
APP_DEBUG=false
APP_SECRET=votre_cle_secrete_application